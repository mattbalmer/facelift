{
    "manifest_version": 2,
    "name": "FaceLift",
    "version": "0.0.8",
    "description": "This extension provides extra utilities to Facebook.",
    "permissions": [
        "https://www.facebook.com/*",
        "storage"
    ],
    "content_scripts" : [ {
        "matches" : [ "https://www.facebook.com/*" ],
        "css" : [ "facelift.css" ]
    } ]
}